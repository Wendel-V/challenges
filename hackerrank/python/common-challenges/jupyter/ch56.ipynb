    "# sWAP cASE\n",
    "\n",
    "def swap_case(s):\n",
    "    swaped = []\n",
    "\n",
    "    for i in range(len(s)):\n",
    "        if s[i].isupper():\n",
    "            swaped.append(s[i].lower())\n",
    "        else:\n",
    "            swaped.append(s[i].upper())\n",
    "    return ''.join(swaped)"
