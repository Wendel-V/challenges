    "# list comprehensions\n",
    "\n",
    "x = 1\n",
    "y = 1\n",
    "z = 2\n",
    "n = 3\n",
    "\n",
    "xlen = len(range(0,x+1))\n",
    "ylen = len(range(0, y+1))\n",
    "zlen = len(range(0, z+1))\n",
    "\n",
    "print([[x, y, z] for x in range(xlen) for y in range(ylen) for z in range(zlen) if x + y + z != n])"
   ]
  }
