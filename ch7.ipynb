    "def miniMaxSum(arr):\n",
    "\n",
    "    result = []\n",
    "    aux = 0\n",
    "\n",
    "    for i in range(5):\n",
    "        x = arr.copy()\n",
    "        del(x[aux])\n",
    "        result.append(sum(x))\n",
    "        aux += 1\n",
    "    \n",
    "    print(str(min(result)) + ' ' + str(max(result)))\n",
    "\n",
    "miniMaxSum(x)"
