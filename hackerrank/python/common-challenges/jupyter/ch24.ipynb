    "def catAndMouse(x, y, z):\n",
    "    dist_a = abs(z - x)\n",
    "    dist_b = abs(z - y)\n",
    "    if dist_a < dist_b:\n",
    "        return 'Cat A'\n",
    "    elif dist_a > dist_b:\n",
    "        return 'Cat B'\n",
    "    else:\n",
    "        return 'Mouse C'\n",
    "\n",
    "print(catAndMouse(2,5,4))\n",
    "print(catAndMouse(1,2,3))\n",
    "print(catAndMouse(1,3,2))"
   ]
  }
