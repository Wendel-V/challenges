   "source": [
    "# String formating\n",
    "\n",
    "\n",
    "\n",
    "def print_formatted(n):\n",
    "    width = len('{0:b}'.format(int(n)))\n",
    "    for i in range(1,n+1):\n",
    "        print('{0:{width}d} {0:{width}o} {0:{width}X} {0:{width}b}'.format(i, width = width))\n",
    "\n",
    "\n"
